{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "inputs": ["$TURBO_DEFAULT$", "../docs/**"],
      "env": ["SPONSOR_GITHUB_TOKEN"],
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        ".content-collections/**",
        ".next/**",
        "!.next/cache/**"
      ]
    }
  }
}
